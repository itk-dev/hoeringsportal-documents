{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "~7.2.0",
        "ext-amqp": "*",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "alterphp/easyadmin-extension-bundle": "^2.0",
        "api-platform/api-pack": "^1.1",
        "cweagans/composer-patches": "^1.6",
        "deskpro/deskpro-api-client-php": "^2.0",
        "easycorp/easyadmin-bundle": "^2.0",
        "friendsofsymfony/user-bundle": "^2.1",
        "itk-dev/database-bundle": "^1.2",
        "itk-dev/edoc-api": "dev-master",
        "kapersoft/sharefile-api": "dev-deltag.aarhus.dk",
        "mpdf/mpdf": "^8.0",
        "opis/json-schema": "^1.0",
        "sensio/framework-extra-bundle": "^5.1",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/amqp-pack": "^1.0",
        "symfony/asset": "4.2.*",
        "symfony/console": "4.2.*",
        "symfony/dotenv": "^4.2",
        "symfony/expression-language": "4.2.*",
        "symfony/flex": "^1.1",
        "symfony/form": "4.2.*",
        "symfony/framework-bundle": "4.2.*",
        "symfony/messenger": "4.2.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "*",
        "symfony/process": "4.2.*",
        "symfony/security-bundle": "4.2.*",
        "symfony/serializer-pack": "*",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/translation": "4.2.*",
        "symfony/twig-bundle": "4.2.*",
        "symfony/validator": "4.2.*",
        "symfony/web-link": "4.2.*",
        "symfony/webpack-encore-bundle": "^1.1",
        "symfony/yaml": "4.2.*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.13",
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.0",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*",
        "symfony/web-server-bundle": "4.2.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "check-coding-standards/php-cs-fixer": [
            "php -n vendor/bin/php-cs-fixer --config=.php_cs.dist fix --dry-run"
        ],
        "check-coding-standards": [
            "@check-coding-standards/php-cs-fixer"
        ],
        "apply-coding-standards/php-cs-fixer": [
            "php -n vendor/bin/php-cs-fixer --config=.php_cs.dist fix"
        ],
        "apply-coding-standards": [
            "@apply-coding-standards/php-cs-fixer"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "patches": {
            "setasign/fpdi": {
                "No destructor": "patches/setasign/fpdi/no-destructor.patch"
            }
        },
        "symfony": {
            "allow-contrib": false,
            "require": "4.2.*"
        }
    },
    "repositories": {
        "itk-dev/edoc-api": {
            "type": "git",
            "url": "https://github.com/itk-dev/edoc-api"
        },
        "kapersoft/sharefile-api": {
            "type": "git",
            "url": "https://github.com/rimi-itk/sharefile-api"
        }
    }
}
