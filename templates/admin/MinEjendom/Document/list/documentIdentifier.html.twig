{% set url = item.data.minejendom.case_url|default(null) %}
{% set filename = item.data.document.data.filename|default(null) %}
{% if url %}
    <a href="{{ url }}">
        {{ value }}
        {% if filename %}
            {{ filename }}{{ item.data.document.data.imageFormat|default('') }}
        {% endif %}
    </a>
{% else %}
    {{ value }}
{% endif %}
